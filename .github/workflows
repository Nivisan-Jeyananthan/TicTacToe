name: Deploy
on:
  push:
    branches: [main]
      jobs:
        build-and-deploy:
          runs-on: ubuntu-latest
          steps:
            - uses: actions/checkout@v2
            - name: Use latest NodeJS
              uses: actions/setup-node@v2
              with:
                node-version: 16
                cache: 'yarn'
           - run: cd tic_tac_toe
           - run: yarn install
           - run: yarn build
           - run 
